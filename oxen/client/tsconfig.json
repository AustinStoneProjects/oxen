{
    "compilerOptions": {
        "outDir": "./built",
        "target": "es6",
        "moduleResolution": "node",
        "pretty": true,
        "allowJs": true,
        "noUnusedLocals": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "jsx": "react"
    },
    "include": ["./src/**/*"]
}
